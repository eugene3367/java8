package sec2;

public class test7 {

	public static void main(String[] args) {
		int max = 0;
		int min = 9;
		
		int[] array ={1,5,3,8,2};
		
		for(int i=0;i<array.length;i++){
			if(array[i]>max){
				max=array[i];
			}
		for(int j=0;j<array.length;j--){
			if(array[j]<min){
				min=array[j];
			}
		}
			
		}
		
		System.out.println("max : "+max);
		System.out.println("min : "+min);

	}

}
