package sec2;

import java.util.Calendar;

public class Ref2 {

	public static void main(String[] args) {
		
		Week today = null;	//내가 지정한거
		
		Calendar cal = Calendar.getInstance();//자바에 있는거
		int w = cal.get(Calendar.DAY_OF_WEEK);
		System.out.println(w);	//1-7(일-토)
		System.out.println();

	}

}
